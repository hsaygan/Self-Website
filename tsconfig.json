{
    "compilerOptions": {
        "target": "esnext",
        "module": "commonjs",
        "jsx": "react",
        "lib": ["dom", "es2015", "es2017"],
        "moduleResolution": "node",
        "strict": true,
        "noEmit": true,
        "skipLibCheck": true,
        "esModuleInterop": true
    },
    "exclude": ["node_modules", "public", ".cache"],
    "include": ["./src"]
}
